{
  "name": "stianscholtz/s3-direct-uploader",
  "description": "A Laravel Package to upload files to s3 directly from the client.",
  "keywords": [
    "stian-scholtz",
    "laravel",
    "file-uploader"
  ],
  "homepage": "https://github.com/stian-scholtz/s3-direct-uploader",
  "license": "MIT",
  "authors": [
    {
      "name": "Stian Scholtz",
      "email": "stian678@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "aws/aws-sdk-php": "^3.279",
    "illuminate/contracts": "^10.0",
    "spatie/laravel-package-tools": "^1.14.0"
  },
  "require-dev": {
    "laravel/pint": "^1.0",
    "nunomaduro/collision": "^7.8",
    "nunomaduro/larastan": "^2.0.1",
    "orchestra/testbench": "^8.8",
    "pestphp/pest": "^2.0",
    "pestphp/pest-plugin-arch": "^2.0",
    "pestphp/pest-plugin-laravel": "^2.0",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-phpunit": "^1.0",
    "spatie/laravel-ray": "^1.26"
  },
  "autoload": {
    "psr-4": {
      "Stianscholtz\\S3DirectUploader\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {}
  },
  "scripts": {
    "post-autoload-dump": "@composer run prepare",
    "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
    "prepare": "@php vendor/bin/testbench package:discover --ansi",
    "build": [
      "@composer run prepare",
      "@php vendor/bin/testbench workbench:build --ansi"
    ],
    "start": [
      "@composer run build",
      "@php vendor/bin/testbench serve"
    ],
    "analyse": "vendor/bin/phpstan analyse",
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage",
    "format": "vendor/bin/pint"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Stianscholtz\\S3DirectUploader\\UploaderServiceProvider"
      ],
      "aliases": {
        "FileUploader": "Stianscholtz\\S3DirectUploader\\Facades\\S3DirectUploader"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
